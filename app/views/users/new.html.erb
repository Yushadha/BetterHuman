<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<%= form_for @user do |f| %>
	<div class="form-group">	
		<%= f.label :first_name %>
		<%= f.text_field :first_name %>
		<br>
	</div>
	<div class="form-group">
		<%= f.label :last_name %>
		<%= f.text_field :last_name %>
		<br>
	</div>
	<div class="form-group">
		<%= f.submit %>
		<%= f.button "Reset", type: :reset %>
	</div>
<% end %>
